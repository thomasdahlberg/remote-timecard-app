<%- include('../partials/header') %>

<h2>Sessions</h2>
<ul class="collapsible">
      <% sessions.forEach(function(m) { %>
        <li class="sites">
          <div class="collapsible-header"><strong><%=m.siteName%>: <%=m.punchClock.timePunch%></strong></div>
          <div class="collapsible-body">
            <div><strong>Geolocation Verified: <%=m.punchClock.verified%></strong></div>
            <div><strong>Proximity: <%=m.punchClock.proximity%>km</strong></div>
          </div>
        <% }); %>
      </ul>



<%- include('../partials/footer') %>
